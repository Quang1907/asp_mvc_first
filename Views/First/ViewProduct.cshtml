@model Product
@{
    ViewData["Title"] = "Product detail";
    var product = ViewData["product"] as Product;
}

<div class="card bg-danger text-white">
    <div class="card-header"><h2>@Model.Name</h2></div>
    <div class="card-body">
        <div class="card-text">
            GIA: @Model.Price
            GIA: @product.Price
        </div>
    </div>
</div>
