@model ASP_MVC.Models.PlanetModel
@{
    ViewData["Title"] = "Detail";
}

<h2>@Model.Name </h2>
 <div class="card w-50">
    <img class="card-img-top" src="~/planet/@(Model.Name).jpg" alt="@(Model.Name)" />
    <div class="card-body">
        <div class="card-text">@Model.Content</div>
        @Url.RouteUrl("neptune")
    </div>
 </div>